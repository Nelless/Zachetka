<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Markusdrop_wpf.Assets.Dictionary">
    <SolidColorBrush x:Key="AverageButtonBrush" Color="LightCyan"></SolidColorBrush>
    <SolidColorBrush x:Key="AuthButtonBrush" Color="Cyan"></SolidColorBrush>
    <SolidColorBrush x:Key="AcceptButtonBrush" Color="SteelBlue"></SolidColorBrush>
    <SolidColorBrush x:Key="BlockBrush" Color="LightSkyBlue"></SolidColorBrush>
    <LinearGradientBrush x:Key="backgroundbrush">
        <GradientStopCollection>
            <GradientStop Color="Aqua" Offset="1" />
            <GradientStop Color="LightBlue" Offset="0.5" />
        </GradientStopCollection>
    </LinearGradientBrush>
</ResourceDictionary>